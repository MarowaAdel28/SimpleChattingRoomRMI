<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane prefHeight="333.0" prefWidth="459.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Button id="startButton" mnemonicParsing="false" onAction="#startServer" prefHeight="31.0" prefWidth="100.0" text="Start" textAlignment="CENTER" textFill="#1d40b5">
         <StackPane.margin>
            <Insets top="-50.0" />
         </StackPane.margin>
      </Button>
      <Button id="stopButton" mnemonicParsing="false" onAction="#stopServer" prefHeight="31.0" prefWidth="98.0" text="Stop" textFill="#1612f2">
         <StackPane.margin>
            <Insets top="100.0" />
         </StackPane.margin>
      </Button>
      <Label alignment="CENTER" prefHeight="34.0" prefWidth="210.0" text="Chating Server App " textAlignment="CENTER" textFill="#1b63d7">
         <StackPane.margin>
            <Insets top="-200.0" />
         </StackPane.margin>
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
   </children>
</StackPane>
